{
    "bin": {
        "pattern": "^[0-1]*$",
        "length": 0
    },
    "dec": {
        "pattern": "^[0-9]*$",
        "length": 0
    },
    "hex": {
        "pattern": "^[a-fA-F0-9]*$",
        "length": 0
    },
    "utf-8": {
        "pattern": "^[\\u0000-\\u007F]*$",
        "length": 0
    },
    "datetime": {
        "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}Z$",
        "length": 56
    },
    "duration": {
        "pattern": "^P[0-9]{2}D[0-9]{2}H[0-9]{2}M$",
        "length": 24
    },
    "lat": {
        "pattern": "^[+-][0-9]{2}.[0-9]{5}$",
        "length": 29
    },
    "long": {
        "pattern": "^[+-][0-9]{3}.[0-9]{5}$",
        "length": 33
    }
}
